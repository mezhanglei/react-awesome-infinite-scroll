{
  "name": "react-awesome-infinite-scroll",
  "version": "0.1.2",
  "description": "简单好用的滚动列表加载组件",
  "main": "lib/bundle.js",
  "repository": "https://github.com/mezhanglei",
  "files": [
    "lib",
    "README.md",
    "README_CN.md"
  ],
  "scripts": {
    "build-typings": "tsc -d --emitDeclarationOnly --target esnext --removeComments false --allowJs false --declarationDir ./lib",
    "build": "cross-env NODE_ENV=production webpack --progress --config ./webpack/webpack.prod.js",
    "stats": "cross-env NODE_ENV=production webpack --progress --config ./webpack/webpack.prod.js --json > stats.json",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/webpack.dev.js --open",
    "eslint": "eslint --ext .js --ext .jsx src/",
    "eslint-fix": "eslint --fix --ext .js --ext src/",
    "csslint": "stylelint src/**/*.{css,scss,less}",
    "css-fix": "stylelint src/**/*.{css,scss,less} --fix"
  },
  "keywords": [
    "react-awesome-infinite-scroll",
    "react-infinite",
    "react-infinite-scroll",
    "infinite-scroll",
    "infinite-list",
    "scroll-list"
  ],
  "author": "zhanglei",
  "license": "MIT",
